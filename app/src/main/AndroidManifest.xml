<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.advse.universitybazaar.register">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserHome" />
        <activity android:name="com.advse.universitybazaar.clubs.ClubHome" />
        <activity android:name="com.advse.universitybazaar.messages.MessageHome" />
        <activity android:name="com.advse.universitybazaar.posts.PostHome" />
        <activity android:name="com.advse.universitybazaar.clubs.CreateClubActivity" />
    </application>

</manifest>